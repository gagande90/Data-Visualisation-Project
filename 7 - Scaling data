<!DOCTYPE html>
<html>
    <head></head>
    <body>
        <script src="../../lib/d3.js"></script>
        <script src="../../lib/d3-selection-multi.v1.js"></script>

        <div id='div1'></div>

        <script type="text/javascript">
            var data = [10, 100, 1000, 10000, 100000]; //to be presented there for mapping position
            var scale = d3.scaleLog()
                .domain([d3.min(data), d3.max(data)])
                .range([0,100]);

            d3.select('#div1')
                .selectAll('div')
                .data(data)
                .enter()
                .append('div')
                .text(function(d){
                    return d + " " + scale(d);
                });
		</script>
    </body>
</html>

